    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css" rel="stylesheet" />
    <title>Get Ready - Coming Soon</title>
</head>

<body>

    <div class="container">
        <section class="image-section">
            <img src="image/7a.png" class="animated-img img1" alt="img1">
            <img src="image/8a.png" class="animated-img img2" alt="img2">
            <img src="image/4a.png" class="animated-img img5" alt="img5">
            <img src="image/6a.png" class="animated-img img9" alt="img9">
            <img src="image/3a.png" class="animated-img img7" alt="img7">
            <!-- <img src="./assets/image/2a.png" class="animated-img img12" alt="img12"> -->
        </section>

        <section class="image-section2">
            <div class="head">
                <div class="headline">GET READY !</div>
                <div class="subheadline">
                    <img src="image/5a.png" class="animated-img img10" alt="img10">
                    SOMETHING AWESOME IS COMING SOON
                </div>

                <form class="subscribe-form" action="/commingsoon" method="post">
                    <div class="custom-input-box" id="custom-input-box">
                        <i class="bi bi-envelope"></i>
                        <input type="email" id="email" name="email" required />
                    </div>
                    <button type="submit" id="subscribe" class="subscribe">SUBSCRIBE</button>
                </form>
                
            </div>
            <div class="im">
                <img src="image/2a.png" class="animated-img img12" alt="img12">
            </div>
        </section>

        <section class="image-section3">
            <img src="image/18a.png" class="animated-img img3" alt="img3">
            <img src="image/14a.png" class="animated-img img11" alt="img11">
            <img src="image/15a.png" class="animated-img img6" alt="img6">
            <img src="image/16a.png" class="animated-img img4" alt="img4">
            <img src="image/10a.png" class="animated-img img8" alt="img8">
        </section>
    </div>

</body>
</html>